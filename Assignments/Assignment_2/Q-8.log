function validatePhone(Num){
    var regex = /^(\([0-9]{3}\) |[0-9]{3}-)[0-9]{3}-[0-9]{4}/;
    var phoneNum =  Num.match(regex);
    if(phoneNum){
        console.log("Valid");
    }
    else{
        console.log("Invalid");
    }
}
function main(){
    var Num = prompt("Enter the Num");
    validatePhone(Num);
}
main();
