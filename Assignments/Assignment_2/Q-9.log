function validateEmail(email){
    var regex = /[a-z0-9]+@[a-z]+\.[a-z]{2,3}/;
    var vemail = email.match(regex);
    if(vemail){
        console,log("Valid Email");
    }else{
        console.log("Invalid Email");
    }
}
function main(){
    var email = prompt("Enter Your Mail");
    validateEmail(email);
}
