function sum(n){
    var rev = reverse(n);
    //console.log("-------");
    //console.log(rev);
    var oddSum =0;
    var evenSum=0;
    while(rev!=0){
        var o_rem = (rev % 10);
        oddSum = oddSum+o_rem;
        rev = rev/10;
        if(rev!=0){
        var e_rem = rev % 10;
        evenSum = evenSum+e_rem;
        rev = rev/10;
        }
    }
    console.log("Odd place sum: "+parseInt(oddSum));
    console.log("even place sum: "+parseInt(evenSum));
}
